{
  "version": 2,
  "builds": [
    {
      "src": "vercel_wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11",
        "maxLambdaSize": "50mb",
        "memory": 1024,
        "maxDuration": 30,
        "pipInstallArgs": ["-r", "requirements.vercel.txt"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "vercel_wsgi.py"
    },
    {
      "src": "/admin/(.*)",
      "dest": "vercel_wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "vercel_wsgi.py"
    },
    {
      "src": "/media/(.*)",
      "dest": "vercel_wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "vercel_wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "dashboard.settings",
    "PYTHONPATH": ".",
    "DJANGO_USE_LIGHTWEIGHT": "true",
    "ALLOWED_HOSTS": ".vercel.app,hrms-final-delta.vercel.app,hrms-final-2ct8.vercel.app,localhost,127.0.0.1,testserver",
    "CORS_ALLOWED_ORIGINS": "https://hrms-final-2ct8.vercel.app",
    "DATABASE_URL": "postgresql://neondb_owner:npg_kiW2lJnVcsu8@ep-lingering-block-a1olkbv3-pooler.ap-southeast-1.aws.neon.tech:5432/neondb?sslmode=require",
    "DB_NAME": "neondb",
    "DB_USER": "neondb_owner",
    "DB_PASSWORD": "npg_kiW2lJnVcsu8",
    "DB_HOST": "ep-lingering-block-a1olkbv3-pooler.ap-southeast-1.aws.neon.tech",
    "DB_PORT": "5432",
    "DEBUG": "False"
  }
}