{
  "version": 2,
  "builds": [
    {
      "src": "vercel_wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11",
        "maxLambdaSize": "50mb",
        "memory": 1024,
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "vercel_wsgi.py"
    },
    {
      "src": "/admin/(.*)",
      "dest": "vercel_wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "vercel_wsgi.py"
    },
    {
      "src": "/media/(.*)",
      "dest": "vercel_wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "vercel_wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "dashboard.settings",
    "PYTHONPATH": ".",
    "DJANGO_USE_LIGHTWEIGHT": "true"
  }
}